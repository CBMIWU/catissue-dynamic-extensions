<?xml version="1.0" encoding="UTF-8"?>

<Primitive-Attributes>
    <Primitive-Attribute>
		  <name>String</name>
	  	  <database-datatype>varchar(8000)</database-datatype>
    </Primitive-Attribute>
    
    <Primitive-Attribute>
		  <name>Boolean</name>
	  	  <database-datatype>bit</database-datatype>
    </Primitive-Attribute>
    
     <Primitive-Attribute>
		  <name>Integer</name>
	  	  <database-datatype>numeric(38,0)</database-datatype>
	  	  <digits-before-decimal>37</digits-before-decimal>
	  	  <digits-after-decimal>0</digits-after-decimal>
    </Primitive-Attribute>
    
     <Primitive-Attribute>
		  <name>Long</name>
	  	  <database-datatype>numeric(38)</database-datatype>
	  	  <digits-before-decimal>37</digits-before-decimal>
	  	  <digits-after-decimal>0</digits-after-decimal>
    </Primitive-Attribute>
    
    <Primitive-Attribute>
		  <name>Double</name>
	  	  <database-datatype>DOUBLE PRECISION</database-datatype>
	  	  <digits-before-decimal>50</digits-before-decimal>
	  	  <digits-after-decimal>15</digits-after-decimal>
    </Primitive-Attribute>
    
    <Primitive-Attribute>
		  <name>Float</name>
	  	  <database-datatype>float</database-datatype>
	  	  <digits-before-decimal>32</digits-before-decimal>
	  	  <digits-after-decimal>6</digits-after-decimal>
    </Primitive-Attribute>
    
    <Primitive-Attribute>
		  <name>Short</name>
	  	  <database-datatype>smallint</database-datatype>
	  	  <digits-before-decimal>18</digits-before-decimal>
	  	  <digits-after-decimal>0</digits-after-decimal>
    </Primitive-Attribute>
    
     <Primitive-Attribute>
		  <name>Date</name>
	  	  <database-datatype>smalldatetime</database-datatype>
    </Primitive-Attribute>
    
    <Primitive-Attribute>
		  <name>DateTime</name>
	  	  <database-datatype>datetime</database-datatype>
    </Primitive-Attribute>
    
    <Primitive-Attribute>
		  <name>File</name>
	  	  <database-datatype>image</database-datatype>
    </Primitive-Attribute>
    
        <Primitive-Attribute>
		  <name>Object</name>
	  	  <database-datatype>varbinary(MAX)</database-datatype>
    </Primitive-Attribute>
    
</Primitive-Attributes>
