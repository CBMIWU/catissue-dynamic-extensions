<?xml version="1.0"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 2.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping default-lazy="false">
    <class
        name="edu.common.dynamicextensions.domain.integration.AbstractRecordEntry"
        table="DYEXTN_ABSTRACT_RECORD_ENTRY"
        dynamic-update="false"
        dynamic-insert="false"
    >

        <id
            name="id"
            column="IDENTIFIER"
            type="long"
            unsaved-value="null"
        >
            <generator class="native">
                <param name="sequence">DYEXTN_ABSTRACT_RE_SEQ</param>
            </generator>
        </id>

        <many-to-one
            name="formContext"
            class="edu.common.dynamicextensions.domain.integration.AbstractFormContext"
            cascade="none"
            outer-join="auto"
            update="true"
            insert="true"
            column="ABSTRACT_FORM_CONTEXT_ID"
        />

        <property
            name="activityStatus"
            type="string"
            update="true"
            insert="true"
            column="ACTIVITY_STATUS"
            length="10"
        />

        <property
            name="modifiedBy"
            type="string"
            update="true"
            insert="true"
            column="MODIFIED_BY"
            length="255"
        />

         <property
            name="modifiedDate"
            type="date"
            update="true"
            insert="true"
            column="MODIFIED_DATE"
        />
        
        
         <property
            name="recordId"
            type="long"
            update="true"
            insert="true"
            column="RECORD_ID"
        />

    </class>

</hibernate-mapping>
